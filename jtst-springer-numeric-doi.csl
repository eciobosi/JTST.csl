<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0">
  <info>
    <title>JTST (Springer Numeric with DOI)</title>
    <id>http://www.zotero.org/styles/jtst-springer-numeric-doi</id>
    <link href="http://www.zotero.org/styles/jtst-springer-numeric-doi" rel="self"/>
    <author>
      <name>Adapted by ChatGPT</name>
    </author>
    <category citation-format="numeric"/>
    <category field="engineering"/>
    <summary>Numeric citation style for JTST with DOI always included.</summary>
    <updated>2025-07-01T00:00:00+00:00</updated>
  </info>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter=", " prefix="(" suffix=")">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography et-al-min="10" et-al-use-first="6" second-field-align="flush">
    <layout>
      <text variable="author" suffix=", "/>
      <text variable="title" suffix=", "/>
      <text variable="container-title" font-style="italic" suffix=", "/>
      <group delimiter=", ">
        <text variable="publisher"/>
        <text variable="publisher-place"/>
        <text variable="issued"/>
        <text variable="volume"/>
        <text variable="issue"/>
        <text variable="page"/>
      </group>
      <text variable="DOI" prefix=", doi:"/>
    </layout>
  </bibliography>
</style>
